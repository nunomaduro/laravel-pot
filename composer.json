{
    "name": "nunomaduro/laravel-pot",
    "description": "Set of commands to inspect Laravel's container",
    "keywords": ["laravel", "package", "container"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^9.34.0"
    },
    "require-dev": {
        "laravel/pint": "^1.2.0",
        "orchestra/testbench": "^7.9",
        "pestphp/pest": "^1.22.1",
        "phpstan/phpstan": "^1.8.7",
        "symfony/var-dumper": "^6.2.0"
    },
    "autoload": {
        "psr-4": {
            "NunoMaduro\\LaravelPot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "lint": "pint -v",
        "test:lint": "pint --test -v",
        "test:types": "phpstan analyse --ansi",
        "test:unit": "pest --colors=always",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "NunoMaduro\\LaravelPot\\PotServiceProvider"
            ]
        }
    }
}
